ITLE       CODE KESIMLERI BIRLESIK YARDIMCI YORDAM CAGIRMA

;WORD TANIMLI, POZITIF SAYILARDAN OLUSAN,10 ELEMANLI BIR DIZININ
;EN BUYUK ELEMANI BUYUK ISIMLI HARICI BIR YIRDAM YARDIMIYLA BULAN
; PROGRAM

EXTRN BUYUK:FAR
PUBLIC DIZI

;BUYUK HARICI BIR YORDAMDIR, FAR TANIMLANMISTIR
;DIZI BASKA BIR PROGRAM TARAFINDAN KULLANILACAKTIR

STACKSG     SEGMENT PARA STACK 'STACK'
            DW 32 DUP(0)
STACKSG     ENDS
DATASG      SEGMENT PARA 'DATA'
            DIZI DW 12,1,5,64,21,65,127,512,0,1024
            ELEMAN DW 10
            BYK_ELM ?
DATASG      ENDS
CODESG      SEGMENT PARA PUBLIC 'CODE'       ;KESIM BIRLESTIRILECEK
            ASSUME SS:STACKSG, DS:DATASG, CS:CODESG
BASLA       PROC FAR
            
            
            PUSH DS
            XOR AX,AX
            PUSH AX
            
            MOV AX,DATASG
            MOV DS,AX
            
            MOV CX,ELEMAN
            CALL BUYUK
            MOV BYK_ELM,AX
            
            
            
            
            RET
BASLA       ENDP
CODESG      ENDS
            END     BASLA
            
